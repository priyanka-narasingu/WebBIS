.allowscroll {
    overflow: auto;
    height:300px;
}

.tt-suggestion {
    padding: 3px 20px;
    font-size: 16px;
    line-height: 20px;
    overflow-y: auto;
}

.tt-dropdown-menu {
    width: 400px;
    margin-top: 12px;
    padding: 8px 0;
    background-color: white;
    border: 1px grey;    
    border-radius: 8px;
    box-shadow: 0 5px 10px grey;
}

.tt-cursor{
    color: white;
    background-color: green;
}